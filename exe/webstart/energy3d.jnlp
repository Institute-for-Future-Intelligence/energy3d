<?xml version="1.0" encoding="utf-8"?>
<jnlp codebase="http://energy.concord.org/energy3d/" href="energy3d.jnlp">
	<information>
		<title>Energy3D</title>
		<vendor>The Concord Consortium</vendor>
		<homepage href="http://energy.concord.org/energy3d" />
		<description>Energy3D</description>
		<description kind="short">An educational CAD tool for green building design.</description>
		<icon href="resources/icon.gif" />
		<icon kind="splash" href="resources/splash.png" /> 
		<offline-allowed/>
		<association extensions="ng3" mime-type="application/energy3d"/>
	    <shortcut>
			<desktop/>
	    </shortcut>
	</information>
	<security>
		<all-permissions/>
	</security>
	<resources>
		<j2se href="http://java.sun.com/products/autodl/j2se" version="1.6+" />
		<property name="sun.java2d.noddraw" value="true" />
		<jar href="energy3d.jar" main="true" />
		<jar href="resources/guava-r09.jar" />
		<jar href="resources/jdom.jar" />

		<jar href="resources/jar/jogl-all.jar" />
		<jar href="resources/jar/gluegen-rt.jar" />
	</resources>

	<resources os="Windows" arch="x86">
		<nativelib href = "resources/jar/jogl-all-natives-windows-i586.jar" />
		<nativelib href = "resources/jar/gluegen-rt-natives-windows-i586.jar" />
	</resources>
	<resources os="Windows" arch="amd64">
		<nativelib href = "resources/jar/jogl-all-natives-windows-amd64.jar" />
		<nativelib href = "resources/jar/gluegen-rt-natives-windows-amd64.jar" />
	</resources>
	<resources os="Windows" arch="x86_64">
		<nativelib href = "resources/jar/jogl-all-natives-windows-amd64.jar" />
		<nativelib href = "resources/jar/gluegen-rt-natives-windows-amd64.jar" />
	</resources>
	<resources os="Linux" arch="i386">
		<nativelib href = "resources/jar/jogl-all-natives-linux-i586.jar" />
		<nativelib href = "resources/jar/gluegen-rt-natives-linux-i586.jar" />
	</resources>
	<resources os="Linux" arch="x86">
		<nativelib href = "resources/jar/jogl-all-natives-linux-i586.jar" />
		<nativelib href = "resources/jar/gluegen-rt-natives-linux-i586.jar" />
	</resources>
	<resources os="Linux" arch="amd64">
		<nativelib href = "resources/jar/jogl-all-natives-linux-amd64.jar" />
		<nativelib href = "resources/jar/gluegen-rt-natives-linux-amd64.jar" />
	</resources>
	<resources os="Linux" arch="x86_64">
		<nativelib href = "resources/jar/jogl-all-natives-linux-amd64.jar" />
		<nativelib href = "resources/jar/gluegen-rt-natives-linux-amd64.jar" />
	</resources>
	<resources os="Mac OS X" arch="i386">
		<nativelib href = "resources/jar/jogl-all-natives-macosx-universal.jar" />
		<nativelib href = "resources/jar/gluegen-rt-natives-macosx-universal.jar" />
	</resources>
	<resources os="Mac OS X" arch="x86_64">
		<nativelib href = "resources/jar/jogl-all-natives-macosx-universal.jar" />
		<nativelib href = "resources/jar/gluegen-rt-natives-macosx-universal.jar" />
	</resources>

	<application-desc main-class="org.concord.energy3d.MainApplication" />
</jnlp>